{
  "functions": {
    "vercel.py": {
      "runtime": "python3.9"
    }
  },
    "builds": [
        {
            "src": "manage.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "manage.py"
        }
    ]
}
